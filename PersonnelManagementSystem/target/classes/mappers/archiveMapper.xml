<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.csqf.personnelmanagementsys.mapper.IArchiveDao">
    <sql id="selectAll">
        select * from t_employee_archives
    </sql>

    <select id="getAllArchive" resultType="archive">
        <include refid="selectAll"/>
    </select>

    <select id="getArchiveByPKId" resultType="archive">
        <include refid="selectAll"/> where pk_aid = #{aid}
    </select>

</mapper>